  <!--================ confirmation part start =================-->
  <section class="confirmation_part padding_top">
    <div class="container">
      <div class="row">
        <div class="col-lg-12">
          <div class="confirmation_tittle">
            <span>Thank you. Your order has been received.</span>
          </div>
        </div>
        <div class="col-lg-6 col-lx-4">
          <div class="single_confirmation_details">
            <h4>order info</h4>
            <ul>
              <li>
                <p>order Id</p><span>: {{orderId}}</span>
              </li>
              <li>
                <p>date</p><span>:{{date}}</span>
              </li>
              <li>
                <p>mayment methord</p><span>: {{paymentMethod}}</span>
              </li>
            </ul>
          </div>
        </div>
        <div class="col-lg-6 col-lx-4">
          <div class="single_confirmation_details">
            <h4>Billing Address</h4>
            
            <ul>
              <li>
                <p>District</p><span>: {{delivery.district}}</span>
              </li>
              <li>
                <p>State</p><span>: {{delivery.state}}</span>
              </li>
              <li>
                <p>country</p><span>: {{delivery.country}}</span>
              </li>
              <li>
                <p>Pincode</p><span>: {{delivery.pincode}}</span>
              </li>
            </ul>
            
          </div>
        </div>
        <div class="col-lg-6 col-lx-4">
          <div class="single_confirmation_details">
            <h4>shipping Address</h4>
            <ul>
              <li>
                <p>Address</p><span>: {{delivery.address}}</span>
              </li>
              <li>
               <p>District</p><span>: {{delivery.district}}</span>
              </li>
              <li>
                <p>State</p><span>: {{delivery.state}}</span>
              </li>
              <li>
               <p>country</p><span>: {{delivery.country}}</span>
              </li>
               <li>
                <p>Pincode</p><span>: {{delivery.pincode}}</span>
              </li>
            </ul>
          </div>
        </div>
      </div>
      <div class="row">
        <div class="col-lg-12">
          <div class="order_details_iner">
            <h3>Order Details</h3>
            <table class="table table-borderless">
              <thead>
                <tr>
                  <th scope="col" colspan="2">Product</th>
                  <th scope="col">Quantity</th>
                  <th scope="col">Total</th>
                </tr>
              </thead>
              <tbody>
                {{#each product}}
                <tr>
                  <th colspan="2"><span>{{name}}</span></th>
                  <th>{{quantity}}</th>
                  <th> <span>₹ {{price}}</span></th>
                </tr>
                {{/each}}
                <tr>
                  <th colspan="3">Total</th>
                  <th> <span>₹ {{grandtotal}}</span></th>
                </tr>
              </tbody>
               
            </table>
            
          </div>
        </div>
      </div>
      <div class="col-lg-12 pt-3">
          <div class="confirmation_tittle">
            <span> <a href="/"><button type="button" class="btn btn-success">continue shopping</button></span>
          </div>
        </div>
    </div>
    
  </section>
  <!--================ confirmation part end =================-->

  <!--::footer_part start::-->
